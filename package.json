{
  "name": "your-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "server": "npm run dev --workspace server",
    "client": "npm run dev --workspace client",
    "install-all": "npm install --workspaces",
    "build-client": "npm run build --prefix client",
    "lint": "eslint . --config eslint.config.js",
    "lint:client": "npx eslint client --config eslint.config.js",
    "lint:server": "npx eslint server --config eslint.config.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run build-client && node -e \"process.env.NODE_ENV='production'; require('./server/index.js')\"",
    "start-no-build": "node -e \"process.env.NODE_ENV='production'; require('./server/index.js')\"",
    "prepare": "npx husky",
    "prisma:generate": "npm run generate --workspace server",
    "postinstall": "npm run prisma:generate && npm run build-client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fs-extra": "^11.3.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1"
  }
}