{
  "name": "your-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "11.x"
  },
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "npm run dev --prefix client",
    "build-client": "npm run build --prefix client",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "lint": "eslint . --config eslint.config.js",
    "lint:client": "eslint client --config eslint.config.js",
    "lint:server": "eslint server --config eslint.config.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run build-client && NODE_ENV=production node server/index.js",
    "start-no-build": "NODE_ENV=production node server/index.js"
  },
